{
  "name": "moddle",
  "version": "4.1.0",
  "description": "A library for importing meta-model based file formats into JS",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "babel -d . src",
    "all": "run-s lint test",
    "lint": "eslint .",
    "dev": "npm test -- --watch",
    "test": "mocha -r esm --reporter=spec test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/moddle"
  },
  "keywords": [
    "model",
    "meta-model",
    "xml",
    "xsd",
    "import",
    "export"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/Nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-plugin-bpmn-io": "^0.4.1",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "min-dash": "^3.0.0"
  }
}
